{
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://localhost:5000"
			},
			"Http2": {
				"Url": "http://localhost:5010",
				"Protocols": "Http2"
			},
			"Https": {
				"Url": "https://localhost:5001"
			},
			"Https2": {
				"Url": "http://localhost:5011",
				"Protocols": "Http2"
			}
		},
		"Limits": {
			"MaxConcurrentConnections": 100,
			"MaxRequestBodySize": 10485760
		}
	},
	"ConnectionStrings": {
		"DefaultConnection": "db:sqlite://Data Source=domaingateway.db",
		"DefaultConnection1": "db:postgres://host=localhost;port=5432;database=domaingateway;user id={{POSTGRES_USER}};password={{POSTGRES_PASSWORD}}"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"DomainGatewayConfiguration": {
		"ClientIdHeaderName": "Client-Id",
		"ExposeConfigurationsEndpoint": true,
		"EnableAdminPortal": false,
		"ConfigurationsEndpointPrefix": "/configurations",
		"UnsecuredHttp2Port": 6000,
		"AutomaticServiceInstanceCleanupInterval": "00:01:00",
		"FileSystemRepository": {
			"ConfigurationsRefreshInterval": "00:00:10",
			"ProxyConfigurationFilePath": "LocalConfigs/proxy-config.json",
			"RateLimiterConfigurationFilePath": "LocalConfigs/rate-limiter-config.json",
			"ServiceDiscoveryConfigurationFilePath": "LocalConfigs/service-discovery-config.json"
		},
		"AwsS3Repository": {
			"ConfigurationsRefreshInterval": "00:01:00",
			"Region": "us-west-2",
			"Bucket": "",
			"Key": "test/domain-gateway-config.json"
		}
	}
}
