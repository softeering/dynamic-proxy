{
	"Kestrel": {
		"EndpointDefaults": {
			"Protocols": "Http2"
		},
		"Endpoints": {
			"Http": {
				"Url": "http://localhost:8001"
			},
			"Https": {
				"Url": "https://localhost:8002"
			}
		},
		"Limits": {
			"MaxConcurrentConnections": 100,
			"MaxRequestBodySize": 10485760
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"ServiceDiscovery": {
		"Client": {
			"BaseUrl": "http://localhost:5001",
			"ClientId": "domain-service-grpc",
			"HttpTimeoutSeconds": 15
		},
		"Registry": {
			"ServiceName": "domain-service-grpc",
			"ServicePort": 8002,
			"ServiceVersion": "1.0.0",
			"HeartBeatIntervalSeconds": 15,
			"DeregisterAfterSeconds": 5
		}
	}
}
