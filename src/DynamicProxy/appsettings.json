{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DynamicProxyConfiguration": {
    "ClientIdHeaderName": "Client-Id",
    "FileSystemRepository": {
      "ProxyConfigurationFilePath": "LocalConfigs/proxy-config.json",
      "ThrottlingConfigurationFilePath": "LocalConfigs/rate-limiter-config.json"
    },
    "AwsS3Repository": {
      "Region": "us-west-2",
      "Bucket": "lde-ingestion-dev",
      "Key": "test/dynamic-proxy-config.json"
    }
  }
}
